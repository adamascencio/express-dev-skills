<%- include('../partials/header') %>
<h1>Skills as of <%= time %></h1>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <form action="/skills/<%= s.id %>?_method=DELETE" method="POST">
        <button type="submit">X</button>
      </form>
      <a href="/skills/<%= s.id %>">
        <%= s.lang %>
      </a>
      -
      <%= s.difficulty %>
    </li>
    <% }); %>
</ul>

<%- include('../partials/footer') %>